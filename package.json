{
  "name": "ollama",
  "version": "0.3.0",
  "description": "An experimental library for the Ollama API",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
        "import": "./dist/mjs/index.js",
        "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test": "jest --config=jest.config.cjs ./test/*",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && node scripts/build-cjs-mjs.js",
    "lint": "eslint ./src/* ./test/*",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://github.com/jmorganca/node-ollama",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmorganca/node-ollama.git"
  },
  "author": "Saul Boyd",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.14",
    "@types/jest": "^29.2.2",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.29.0",
    "eslint-plugin-jest": "^27.1.4",
    "jest": "^29.3.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
